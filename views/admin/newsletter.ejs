<section id="dashbaord">
    <div class="contain-fluid">
      <div class="heading">
        <div class="text">
          <h3>Subscribers</h3>
          <ul>
            <li><a href="">Home</a></li>
            <li class="active"><a href="">Subscribers</a></li>
          </ul>
        </div>
        
      </div>
      <div class="dashTable">
        <table class="dataTable display" id="basic-1">
          <thead>
            <tr class="table-scnd-row blog-table">
              <th width="10%">
                <p>Sr#</p>
              </th>
              <th width="20%">
                <p>Name</p>
              </th>
              <th width="15%">
                <p>Status</p>
              </th>
              <th width="10%">
                <p>Action</p>
              </th>
            </tr>
          </thead>
          <tbody>
            <% if (subscribers && subscribers.length > 0) { %> <%
                subscribers.forEach((subscriber, index) => { %>
            <tr class="bottom-cntnt" id="subscriber-<%= subscriber.id %>">
              <td>
                <p><%= index + 1 %></p>
              </td>
              <td>
                <p><%= subscriber.email %></p>
              </td>
              <td>
                <p>
                  <% if (subscriber.status === 1) { %>
                  <a href="#" class="status badge success">Active</a>
                  <% } else { %>
                  <a href="#" class="status badge warning">Inactive</a>
                  <% } %>
                </p>
              </td>
              <td>
                <div class="btn-group">
                  <button
                    type="button"
                    class="dropdown-toggle"
                    data-toggle="dropdown">
                    <i class="fa fa-ellipsis-h"></i>
                  </button>
                  <ul class="dropdown-menu dropdown-primary" role="menu">
                    
                    <li>
                      <a
                        href="#"
                        class="delete-item"
                        data-id="<%= subscriber.id %>"
                        data-url="/admin/subscribers/delete/<%= subscriber.id %>">
                        <i class="fa fa-trash" aria-hidden="true"></i> Delete
                      </a>
                    </li>
                  </ul>
                </div>
              </td>
            </tr>
            <% }) %> <% } else { %>
            <tr>
              <td colspan="8"><p>No services found.</p></td>
            </tr>
            <% } %>
          </tbody>
        </table>
      </div>
    </div>
  </section>
  