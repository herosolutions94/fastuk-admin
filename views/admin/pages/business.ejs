              <div class="card">
                <div class="card-header">
                  <h5>Page Title & Meta Details</h5>
                </div>
                <div class="card-body">
                  <div class="row">
                    <div class="col-md-12">
                      <div class="txtGrp">
                        <div class="row">
                          <div class="col-md-12">
                            <label class="form-label"
                              >Page Title
                              
                            </label>
                            <input
                              type="text"
                              name="page_title"
                              value="<%= contentData.page_title || '' %>"
                              class="input"
                              required="" />
                          </div>
                        </div>
                      </div>
                      <div class="txtGrp">
                        <div class="row">
                          <div class="col-md-12">
                            <label class="form-label"
                              >Meta Title
                              
                            </label>
                            <input
                              type="text"
                              name="meta_title"
                              value="<%= contentData.meta_title || '' %>"
                              class="input"
                              required="" />
                          </div>
                        </div>
                      </div>
  
                      <div class="txtGrp">
                        <div class="row">
                          <div class="col-md-12">
                            <label class="form-label"
                              >Meta Description<abbr
                                class="required"
                                title="required"
                                >*</abbr
                              >
                            </label>
                            <textarea rows="5" name="meta_desc" class="input"><%= contentData.meta_desc || '' %></textarea>
                          </div>
                        </div>
                      </div>
                      <div class="txtGrp">
                        <div class="row">
                          <div class="col-md-12">
                            <label class="form-label"
                              >Meta Keywords
                              
                            </label>
                            <textarea rows="5" name="meta_keywords" class="input"><%= contentData.meta_keywords || '' %></textarea>
                          </div>
                        </div>
                      </div>
                    </div>
                  </div>
                </div>
              </div>
            </div>
